{
  "name": "test-panda",
  "version": "1.0.0",
  "description": "온라인 시험 플랫폼",
  "private": true,
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=9.0.0"
  },
  "scripts": {
    "dev": "pnpm --filter frontend dev",
    "build": "pnpm --filter frontend build",
    "lint": "pnpm --filter frontend lint",
    "task:done": "bash commit-task.sh",
    "task:list": "cat .speckit/tasks.md | grep -E '^- \\[[ x]\\]'",
    "task:pending": "cat .speckit/tasks.md | grep -E '^- \\[ \\]' | head -10"
  },
